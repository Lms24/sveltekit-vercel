{
  "name": "sveltekit-vercel",
  "version": "0.0.1",
  "scripts": {
    "clean": "rm -rf node_modules .svelte-kit && yarn",
    "dev": "vite dev",
    "build:only": "vite build",
    "build": "rm -rf .svelte-kit .vercel/output && sh copy-sentry-over.sh && sh copy-sentry-into-node-modules.sh && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "deploy": "vercel build && vercel deploy --prebuilt --prod"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^4.5.10",
    "@neoconfetti/svelte": "^1.0.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-vercel": "2",
    "@sveltejs/kit": "^1.5.0",
    "@types/cookie": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-plugin-svelte": "^2.26.0",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vercel": "^29.4.0",
    "vite": "^4.3.0"
  },
  "type": "module",
  "dependencies": {
    "@sentry/sveltekit": "^7.52.1",
    "magicast": "^0.2.8"
  }
}
